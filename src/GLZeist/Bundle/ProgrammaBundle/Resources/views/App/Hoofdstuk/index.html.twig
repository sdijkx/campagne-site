{% extends "GLZeistProgrammaBundle::base.html.twig" %} 
{%block title%}Hoofdstukken{%endblock%}
{%block seo%}
    <meta name="description" content="Hoofdstukken van het verkiezingsprogramma van Groenlinks Afdeling Zeist">
{%endblock%}

{%block content%}

{%for hoofdstuk in hoofdstukken%}
    <div class="hoofdstuk-list-entry list-entry clearfix">
        <h2 class="titel">
            <a title="{{hoofdstuk.titel}}" href="{{path('hoofdstuk',{slug:hoofdstuk.slug})}}">{{hoofdstuk.titel}}</a>
        </h2>
    </div>
{%endfor%}


{%endblock%}

{%block rightcontent%}

{%endblock%}