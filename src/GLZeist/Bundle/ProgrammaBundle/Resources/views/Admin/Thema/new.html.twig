{% extends "GLZeistProgrammaBundle::admin.html.twig" %} 
{%block content%}
<h1>Thema toevoegen</h1>
<ul class="record_actions">
    <li>
        <a href="{{ path('admin_thema') }}">
            Terug naar het overzicht
        </a>
    </li>
</ul>
<form action="{{ path('thema_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <p>
        <button type="submit">Toevoegen</button>
    </p>
</form>
<script>
    require(["ckeditor/ckeditor"], function() {
        CKEDITOR.replace('glzeist_bundle_programmabundle_thematype_tekst');
    });
</script>

{%endblock%}