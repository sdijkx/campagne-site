{% extends "GLZeistProgrammaBundle::admin.html.twig" %} 
{%block content%}

<h1>Trefwoord toevoegen</h1>

<form action="{{ path('trefwoord_create') }}" method="post" {{ form_enctype(form) }}>
    {{form_errors(form)}}
    {{ form_widget(form) }}
    <div class="form_actions">
        <button type="submit">Toevoegen</button>
    </div>
</form>

<ul class="record_actions">
    <li>
        <a href="{{ path('admin_trefwoord') }}">
            Terug naar het overzicht
        </a>
    </li>
</ul>
{%endblock%}