{% extends "GLZeistProgrammaBundle::admin.html.twig" %} {%block content%}<h1>Trefwoord</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Trefwoord</th>
            <td>{{ entity.trefwoord }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('trefwoord') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('trefwoord_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('trefwoord_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
{%endblock%}