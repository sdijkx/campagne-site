{% extends "GLZeistProgrammaBundle::admin.html.twig" %} {%block content%}<h1>Media</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Url</th>
            <td>{{ entity.url }}</td>
        </tr>
        <tr>
            <th>Type</th>
            <td>{{ entity.type }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('media') }}">
            Terug naar het overzicht
        </a>
    </li>
    <li>
        <a href="{{ path('media_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        {{macros.delete_form('media_delete',delete_form,entity)}}
    </li>
</ul>
{%endblock%}